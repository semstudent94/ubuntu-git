//config account gihub

git config --global user.name"username"


//see account
git config --list

//file located
pwd
ls -al

//open file
gedit .gitconfig

//clone reposity from github
git clone "link reposity you wanna to clone"

//to create new file
touch "name of file"

//open file
gedit "name of file"

//chek status
git status

//commit
git add "name of file"

//give commned on commit
git commit -m "your comment" "file name"

//push to git hub
git push -u origin master
